{
  "name": "{{npmName}}",
  "version": "{{npmVersion}}",
  "description": "Client for {{appName}}",
  "author": "{{^removeKonfigBranding}}Konfig{{/removeKonfigBranding}}{{#removeKonfigBranding}}{{infoEmail}}{{/removeKonfigBranding}}",
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://{{gitHost}}/{{gitUserId}}/{{gitRepoId}}"
  },
  "keywords": [
    "axios",
    "typescript",
{{^removeKonfigBranding}}
    "konfig",
{{/removeKonfigBranding}}
    "{{npmName}}"
  ],
  "license": "Unlicense",
  "files": [
    "./dist/**/*"
  ],
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
{{#supportsES6}}
  "module": "./dist/esm/index.js",
  "sideEffects": false,
{{/supportsES6}}
  "scripts": {
    "build": "npm run clean && tsc && webpack{{#supportsES6}} && tsc -p tsconfig.esm.json{{/supportsES6}}",
    "clean": "rm -rf dist/",
    "prepack": "npm run build",
    "test": "jest"
  },
  "dependencies": {
{{{dependencies}}}
  },
  "resolutions": {
    "semver": "^7.5.2",
    "@babel/traverse": "^7.23.2"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "@types/node": "^18",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }{{#npmRepository}},{{/npmRepository}}
{{#npmRepository}}
  "publishConfig": {
    "registry": "{{npmRepository}}"
  }
{{/npmRepository}}
}
